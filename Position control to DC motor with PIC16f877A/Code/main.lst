.................... void lcd_send_byte(unsigned int8 address, unsigned int8 n) 
.................... { 
....................   #if defined(__PCB__) 
....................    set_tris_lcd(LCD_OUTPUT_MAP); 
....................   #else 
....................    lcd_enable_tris(); 
*
00C3:  BSF    03.5
00C4:  BCF    08.0
....................    lcd_rs_tris(); 
00C5:  BCF    08.1
....................    lcd_rw_tris(); 
00C6:  BCF    08.2
....................   #endif 
....................  
....................    lcd_output_rs(0); 
00C7:  BCF    03.5
00C8:  BCF    08.1
....................    while ( bit_test(lcd_read_byte(),7) ) ; 
*
00EA:  MOVF   78,W
00EB:  MOVWF  52
00EC:  BTFSC  52.7
00ED:  GOTO   0C9
....................    lcd_output_rs(address); 
00EE:  BTFSS  50.0
00EF:  BCF    08.1
00F0:  BTFSC  50.0
00F1:  BSF    08.1
....................    delay_cycles(1); 
00F2:  NOP
....................    lcd_output_rw(0); 
00F3:  BCF    08.2
....................    delay_cycles(1); 
00F4:  NOP
....................    lcd_output_enable(0); 
00F5:  BCF    08.0
....................    lcd_send_nibble(n >> 4); 
00F6:  SWAPF  51,W
00F7:  MOVWF  52
00F8:  MOVLW  0F
00F9:  ANDWF  52,F
00FA:  MOVF   52,W
00FB:  MOVWF  53
00FC:  CALL   0B1
....................    lcd_send_nibble(n & 0xf); 
00FD:  MOVF   51,W
00FE:  ANDLW  0F
00FF:  MOVWF  52
0100:  MOVWF  53
0101:  CALL   0B1
0102:  RETURN
.................... } 
