.................... unsigned int8 lcd_read_nibble(void) 
.................... { 
....................   #if (defined(LCD_DATA4) && defined(LCD_DATA5) && defined(LCD_DATA6) && defined(LCD_DATA7)) 
....................    unsigned int8 n = 0x00; 
....................  
....................    /* Read the data port */ 
....................    n |= input(LCD_DATA4); 
....................    n |= input(LCD_DATA5) << 1; 
....................    n |= input(LCD_DATA6) << 2; 
....................    n |= input(LCD_DATA7) << 3; 
....................     
....................    return(n); 
....................   #else 
....................    return(lcd.data); 
*
00BD:  MOVF   08,W
00BE:  MOVWF  77
00BF:  SWAPF  08,W
00C0:  ANDLW  0F
00C1:  MOVWF  78
....................   #endif 
00C2:  RETURN
.................... } 
....................  
.................... void lcd_send_nibble(unsigned int8 n) 
.................... { 
....................   #if (defined(LCD_DATA4) && defined(LCD_DATA5) && defined(LCD_DATA6) && defined(LCD_DATA7)) 
....................    /* Write to the data port */ 
....................    output_bit(LCD_DATA4, bit_test(n, 0)); 
....................    output_bit(LCD_DATA5, bit_test(n, 1)); 
....................    output_bit(LCD_DATA6, bit_test(n, 2)); 
....................    output_bit(LCD_DATA7, bit_test(n, 3)); 
....................   #else       
....................    lcdlat.data = n; 
*
00B1:  SWAPF  53,W
00B2:  ANDLW  F0
00B3:  MOVWF  77
00B4:  MOVLW  0F
00B5:  ANDWF  08,W
00B6:  IORWF  77,W
00B7:  MOVWF  08
....................   #endif 
....................        
....................    delay_cycles(1); 
00B8:  NOP
....................    lcd_output_enable(1); 
00B9:  BSF    08.0
....................    delay_us(2); 
00BA:  GOTO   0BB
....................    lcd_output_enable(0); 
00BB:  BCF    08.0
00BC:  RETURN
.................... } 
